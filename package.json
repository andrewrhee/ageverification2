{
  "name": "age-verification",
  "version": "1.0.0",
  "description": "This is an ecommerce website",
  "main": "dist",
  "scripts": {
    "dev": "NODE_ENV=development nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "NODE_ENV=production pm2 start dist",
    "prestart": "npm run -s build",
    "lint": "eslint src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "author": "Andrew Rhee",
  "license": "ISC",
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.17.0",
    "connect-mongo": "^1.3.2",
    "cookie-parser": "^1.4.3",
    "crypto": "^0.0.3",
    "ejs-mate": "^2.3.0",
    "express": "^4.15.0",
    "express-flash": "0.0.2",
    "express-session": "^1.15.1",
    "faker": "^4.1.0",
    "mongoose": "^4.8.5",
    "morgan": "^1.8.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "stripe": "^4.15.0",
    "twilio": "^2.11.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.22.0",
    "eslint": "^3.18.0",
    "nodemon": "^1.11.0"
  }
}
